<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:imi="http://intelligent-multimodal-interaction.org/relations/"
  xml:base="http://intelligent-multimodal-interaction.org/concepts/">


<rdf:Description rdf:about="CurrentNoise">
	<rdf:type rdf:resource="Noise"/>
	<imi:constraint-value>low</imi:constraint-value>
	<!-- <imi:constraint-value>high</imi:constraint-value> -->
</rdf:Description>


<rdf:Description rdf:about="CurrentSpeed">
	<rdf:type rdf:resource="Speed"/>
	<!-- <imi:constraint-value>idle</imi:constraint-value> -->
	<imi:constraint-value>driving</imi:constraint-value>
</rdf:Description>

<rdf:Description rdf:about="CurrentLineStatus">
	<rdf:type rdf:resource="LineStatus"/>
	<imi:constraint-value>off-line</imi:constraint-value>
	<!-- <imi:constraint-value>on-line</imi:constraint-value> -->
</rdf:Description>

<rdf:Description rdf:about="CurrentBrightness">
	<rdf:type rdf:resource="Brightness"/>
	<!-- <imi:constraint-value>night</imi:constraint-value> -->
	<imi:constraint-value>day</imi:constraint-value>
</rdf:Description>


</rdf:RDF>